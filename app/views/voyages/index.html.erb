<p id="notice"><%= notice %></p>

<h1>Voyages</h1>

<table>
  <thead>
    <tr>
      <th>Vessel</th>
      <th>Number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voyages.each do |voyage| %>
      <tr>
        <td><%= voyage.vessel_id %></td>
        <td><%= voyage.number %></td>
        <td><%= link_to 'Show', voyage %></td>
        <td><%= link_to 'Edit', edit_voyage_path(voyage) %></td>
        <td><%= link_to 'Destroy', voyage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voyage', new_voyage_path %>
